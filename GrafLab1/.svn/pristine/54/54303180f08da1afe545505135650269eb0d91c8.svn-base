using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;


//декартовы координаты
namespace GrafLab1
{
    class DecartCoordinates
    {
        private int x = 0;
        private int y = 0;

        public DecartCoordinates() {
            this.setX(0);
            this.setY(0);
        }

        public DecartCoordinates(int x , int y) {
            this.setX(x);
            this.setY(y);
        }


        public int getX()
        {
            return this.x;

        }

        public void setX(int x)
        {
            this.x = x;
        }

        public int getY()
        {
            return this.y;

        }

        public void setY(int y)
        {
            this.y = y;
        }
    }
}
